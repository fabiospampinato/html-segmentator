{
  "name": "html-segmentator",
  "repository": "github:fabiospampinato/html-segmentator",
  "description": "A small library for splitting an HTML string into its top-level sections. Based on htmlparser2.",
  "version": "2.0.0",
  "type": "module",
  "main": "dist/index.js",
  "exports": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "clean": "tsex clean",
    "compile": "tsex compile",
    "compile:watch": "tsex compile --watch",
    "test": "tsex test",
    "test:watch": "tsex test --watch",
    "prepublishOnly": "npm run clean && npm run compile && npm run test"
  },
  "keywords": [
    "html",
    "segment",
    "segments",
    "segmentator",
    "split",
    "top-level"
  ],
  "dependencies": {
    "domhandler": "^5.0.3"
  },
  "devDependencies": {
    "fava": "^0.0.6",
    "tsex": "^1.1.2",
    "typescript": "^4.7.4"
  }
}
